{
  "name": "sfdx-build",
  "description": "sfdx plugin for creating a package.xml",
  "version": "0.0.2",
  "author": "ahmed @abdokhaire",
  "bugs": "https://github.com/abdokhaire/sfdx-build/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^1.5.3",
    "@salesforce/core": "^1.3.3",
    "cerialize": "^0.1.18",
    "tslib": "^1.10.0",
    "xml-js": "^1.6.7",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.4",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^12.0.8",
    "chai": "^4",
    "globby": "^9.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sinon": "5",
    "ts-node": "^8.3.0",
    "typescript": "~3.5.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/abdokhaire/sfdx-build",
  "keywords": [
    "sfdx-plugin",
    "salesforce",
    "sfdc",
    "sfdx",
    "typescript",
    "sfdx-build",
    "oclif"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "abdokhaire/sfdx-build",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
